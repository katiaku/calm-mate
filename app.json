{
  "expo": {
    "name": "calm-mate",
    "slug": "calm-mate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "calmmate",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/cute-rhino-meditation.avif",
      "resizeMode": "contain",
      "backgroundColor": "#E2E1EF"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-font"]
  }
}
